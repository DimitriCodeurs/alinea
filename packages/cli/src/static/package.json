{
  "private": true,
  "version": "0.0.0",
  "name": "@alinea/content",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./store.js": {
      "require": "./store.cjs",
      "default": "./store.js"
    },
    "./config.js": "./config.js",
    "./backend.js": {
      "require": "./backend.cjs",
      "default": "./backend.js"
    },
    "./.server/*": {
      "worker": "./.server/dist/*",
      "browser": "./.server/client.cjs",
      "default": "./.server/dist/*"
    }
  }
}
